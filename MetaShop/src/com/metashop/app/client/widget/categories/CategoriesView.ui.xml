<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b3='urn:import:org.gwtbootstrap3.client.ui'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel addStyleNames="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title">
				<b3:Anchor ui:field="anchor" dataToggle="COLLAPSE" dataParent="#accordian" href="#sportswear">
					<span class="badge pull-right">
						<i ui:field="cross" class="fa fa-plus"></i>
					</span>
					<!-- sportwear -->
				</b3:Anchor>
			</h4>
		</div>
		<g:HTMLPanel ui:field="subpanel" addStyleNames="panel-collapse collapse">
			<div class="panel-body">
				<ul ui:field="brands">
					<li>
						<a href="#">Nike </a>
					</li>
					<li>
						<a href="#">Under Armour </a>
					</li>
					<li>
						<a href="#">Adidas </a>
					</li>
					<li>
						<a href="#">Puma</a>
					</li>
					<li>
						<a href="#">ASICS </a>
					</li>
				</ul>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder>
