<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b3='urn:import:org.gwtbootstrap3.client.ui'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel addStyleNames="col-sm-4">
		<div class="product-image-wrapper">
			<div class="single-products">
				<div class="productinfo text-center">
					<img ui:field="image" src="images/home/product5.jpg" alt="" />
					<h2 ui:field="price1">$56</h2>
					<p ui:field="name1">Easy Polo Black Edition</p>
					<a href="#" class="btn btn-default add-to-cart">
						<i class="fa fa-shopping-cart"></i>
						Add to cart
					</a>
				</div>
				<div class="product-overlay">
					<div class="overlay-content">
						<h2 ui:field="price2">$56</h2>
						<p ui:field="name2">Easy Polo Black Edition</p>
						<a href="#" class="btn btn-default add-to-cart">
							<i class="fa fa-shopping-cart"></i>
							Add to cart
						</a>
					</div>
				</div>
				<img ui:field="sale" src="images/home/sale.png" class="new" alt="" />
				<img ui:field="newItem" src="images/home/new.png" class="new" alt="" />
			</div>
			<div class="choose">
				<ul class="nav nav-pills nav-justified">
					<li>
						<a href="#">
							<i class="fa fa-plus-square"></i>
							Add to wishlist
						</a>
					</li>
					<li>
						<a href="#">
							<i class="fa fa-plus-square"></i>
							Add to compare
						</a>
					</li>
				</ul>
			</div>
		</div>
	</g:HTMLPanel>

</ui:UiBinder>
